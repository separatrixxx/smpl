{
  "name": "smpl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "prisma generate && prisma migrate deploy && next build --webpack",
    "start": "next start --webpack",
    "lint": "next lint",
    "stylelint": "stylelint \"**/*.scss\" --fix",
    "check": "stylelint \"**/*.scss\" --fix && next lint && next build",
    "migrate": "npx prisma migrate dev && npx prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "next": "^16.1.5",
    "pg": "^8.17.2",
    "prisma": "^7.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@types/classnames": "^2.3.4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axios": "^1.8.3",
    "date-fns": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "postcss-scss": "^4.0.9",
    "sass": "^1.85.1",
    "stylelint": "^16.16.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.1",
    "swr": "^2.3.3",
    "typescript": "^5",
    "zustand": "^5.0.3"
  }
}
