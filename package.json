{
  "name": "smpl",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "prisma"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "test": "vitest run",
    "migrate:deploy": "prisma migrate deploy",
    "lint": "eslint src",
    "stylelint": "stylelint \"**/*.scss\"",
    "check": "pnpm run stylelint && pnpm run lint --fix",
    "migrate": "pnpm prisma migrate dev && pnpm prisma generate",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@prisma/client-runtime-utils": "^7.3.0",
    "next": "^16.1.5",
    "next-rspack": "^16.1.6",
    "pg": "^8.17.2",
    "prisma": "^7.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vercel/analytics": "^1.6.1",
    "axios": "^1.8.3",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.3",
    "postcss-scss": "^4.0.9",
    "sass": "^1.85.1",
    "stylelint": "^16.16.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.1",
    "swr": "^2.3.3",
    "typescript": "^5",
    "vitest": "^4.0.18",
    "zustand": "^5.0.3"
  }
}